{
    "env": {
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-console": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "class-methods-use-this": "off",
      "no-unreachable": "off",
      "no-unused-vars": "warn"
    }
}
